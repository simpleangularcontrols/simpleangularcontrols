{
  "name": "sac-controls",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "demo-bs4": "ng serve demo-bootstrap4",
    "build-common": "ng build --project sac-common",
    "build-bs3": "ng build --project sac-bootstrap3",
    "build-bs4": "ng build --project sac-bootstrap4",
    "run-common": "ng build --project sac-common --watch",
    "run-bs3": "ng build --project sac-bootstrap3 --watch",
    "run-bs4": "ng build --project sac-bootstrap4 --watch",
    "lint-common": "ng lint sac-common",
    "lint-bs4": "ng lint sac-bootstrap4",
    "compodoc-bs3": "npx compodoc -c bootstrap3.compodocrc.json",
    "compodoc-bs4": "npx compodoc -c bootstrap4.compodocrc.json",
    "doc-bs3": "start http://localhost:8888 & npx compodoc -r 8888 -s -d ../docs/bootstrap3",
    "doc-bs4": "start http://localhost:8888 & npx compodoc -r 8888 -s -d ../docs/bootstrap4",
    "test-edit": "npx cypress open --project projects/sac-bootstrap4 --env coverage=true",
    "test-bs4": "npm run build-common && npm run instrument-common && npx cypress run --component --project projects/sac-bootstrap4 --env coverage=true",
    "ngcc": "ngcc",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "instrument-common": "npx nyc instrument ./dist/sac-common --in-place"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/localize": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "@tinymce/tinymce-angular": "5.0.0",
    "angular-imask": "6.2.2",
    "core-js": "3.6.5",
    "moment": "2.29.1",
    "ngx-toastr": "14.2.1",
    "ngx-uploadx": "5.0.0",
    "rxjs": "^6.6.3",
    "rxjs-compat": "^6.5.5",
    "tinymce": "^5.8.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.0",
    "@angular-devkit/core": "~13.2.0",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/schematics": "13.5.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "~13.2.0",
    "@angular/compiler-cli": "~13.2.0",
    "@angular/language-service": "~13.2.0",
    "@compodoc/compodoc": "1.1.23",
    "@cypress/code-coverage": "^3.12.8",
    "@cypress/schematic": "2.4.0",
    "@types/node": "^14.0.5",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "codelyzer": "^6.0.0",
    "cypress": "^12.5.1",
    "eslint": "^8.17.0",
    "ng-packagr": "~13.2.0",
    "tslint": "~6.1.0",
    "typescript": "~4.4.2"
  }
}